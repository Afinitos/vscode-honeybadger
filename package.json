{
  "name": "vscode-honeybadger",
  "displayName": "Honeybadger errors",
  "description": "This unofficial extension integrates Honeybadger into VS Code",
  "version": "0.2.0",
  "publisher": "Afinitos",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
    "babel": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-typescript"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-runtime"
      ]
  },
  "main": "./out/extension.js",
  "repository": {
    "url": "https://github.com/Afinitos/vscode-honeybadger.git"
  },
  "contributes": {
    "commands": [
      {
        "command": "hbProjects.refreshEntry",
        "title": "Refresh"
      },
      {
        "command": "vscode-honeybadger.refreshEntry",
        "title": "Refresh"
      }
    ],
    "views": {
      "hb-explorer": [
        {
          "type": "webview",
          "id": "hbProjects",
          "name": "Projects",
          "icon": "media/hbicon.svg",
          "contextualTitle": "Projects"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "hbProjects.refreshEntry",
          "when": "view == hbProjects",
          "group": "navigation"
        },
        {
          "command": "vscode-honeybadger.refreshEntry",
          "when": "view == vscode-honeybadger",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "hb-explorer",
          "title": "Faults in Honeybadger",
          "icon": "media/hbicon.svg"
        }
      ]
    },
    "configuration": {
      "title": "Honeybadger Errors",
      "properties": {
        "honeybadger.apiKey": {
          "type": "string",
          "default": "",
          "description": "API key."
        },
        "honeybadger.apiRootURL": {
          "type": "string",
          "default": "https://app.honeybadger.io",
          "description": "API root URL."
        }
      }
    }
  },
  "scripts": {
    "dev": "nodemon --watch src/webviews/ -e 'ts,tsx,css' --exec snowpack build",
    "vscode:prepublish": "run-s clean compile",
    "clean": "rm -rf out dist",
    "compile": "run-s webpack:build webview:build",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build": "TAILWIND_MODE='build' snowpack build",
    "webpack:build": "webpack",
    "webview:build": "TAILWIND_MODE='build' snowpack build"
  },
  "devDependencies": {
    "@snowpack/plugin-dotenv": "^2.2.0",
    "@snowpack/plugin-postcss": "^1.4.3",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/snowpack-env": "^2.3.5",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.3.5",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@babel/core": "^7.23.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "autoprefixer": "^10.4.16",
    "axios": "^1.4.0",
    "babel-loader": "^9.1.3",
    "init": "^0.1.2",
    "moment": "^2.29.4",
    "nodemon": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "snowpack": "^3.8.8",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
